#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
	int row, column, rw, cl;
	
	cout << "How many rows: ";
	cin >> row;
	cout << "How many columns: ";
	cin >> column;
	
	for (rw=1; rw<=row; rw++)
	{
		for (cl=1; cl<=column; cl++)
		{
			cout << "*";
		}
		cout << "\n";
	}
	
	_getch();
	return 0;
}
